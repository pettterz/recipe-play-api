# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET /recipes   controllers.RecipeController.index
GET /recipes/:id   controllers.RecipeController.show(id: Long)
POST /recipes  controllers.RecipeController.create
PATCH /recipes/:id   controllers.RecipeController.update(id: Long)
DELETE /recipes/:id   controllers.RecipeController.delete(id: Long)

